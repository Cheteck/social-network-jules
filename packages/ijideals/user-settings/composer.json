{
    "name": "ijideals/user-settings",
    "description": "A Laravel package to manage user-specific settings.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jules AI",
            "email": "jules@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ijideals\\UserSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ijideals\\UserSettings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijideals\\UserSettings\\Providers\\UserSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
