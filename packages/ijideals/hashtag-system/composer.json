{
    "name": "ijideals/hashtag-system",
    "description": "A Laravel package to add hashtag functionality to models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jules Agent",
            "email": "jules@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ijideals\\HashtagSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ijideals\\HashtagSystem\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijideals\\HashtagSystem\\Providers\\HashtagSystemServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
