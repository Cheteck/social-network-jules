{
    "name": "ijideals/social-posts",
    "description": "A Laravel package to manage social posts, comments, and likes.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "IJIDeals Collaborator (Jules)",
            "email": "jules@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ijideals\\SocialPosts\\": "src/",
            "Ijideals\\SocialPosts\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijideals\\SocialPosts\\Providers\\SocialPostsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
