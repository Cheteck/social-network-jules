{
    "name": "ijideals/notification-system",
    "description": "A Laravel package for handling user notifications.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Jules AI",
            "email": "jules@ai.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "illuminate/notifications": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ijideals\\NotificationSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ijideals\\NotificationSystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijideals\\NotificationSystem\\Providers\\NotificationSystemServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
