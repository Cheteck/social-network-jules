{
    "name": "ijideals/followable",
    "description": "A package to add followable functionality to Laravel models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jules",
            "email": "jules@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ijideals\\Followable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijideals\\Followable\\FollowableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
